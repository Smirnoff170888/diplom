{
    "env": {
        "browser": true,
        "es6": true
    },
    "globals": {
        "config": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "max-len": ["warn", {"code": 120, "comments": 150}],
        "semi": ["error", "always"],
        "indent": ["error", 4]
    },
    "extends": ["eslint:recommended"]
}